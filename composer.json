{
    "name": "/yii2-asset-advancedhosters",
    "type": "yii2-extension",
    "description": "AdvancedHosters asset files: images, configs and more",
    "keywords": [
        "advancedhosters",
        "asset",
        "extension",
        "yii2"
    ],
    "homepage": "https://github.com//yii2-asset-advancedhosters",
    "license": "No license",
    "support": {
        "source": "https://github.com/advancedhosters/yii2-asset-advancedhosters",
        "issues": "https://github.com/advancedhosters/yii2-asset-advancedhosters/issues",
        "wiki": "https://github.com/advancedhosters/yii2-asset-advancedhosters/wiki",
        "email": "support@advancedhosters.com"
    },
    "authors": [],
    "require-dev": {
        "hiqdev/hidev-php": "<2.0",
        "advancedhosters/hidev-vendor": "*"
    },
    "autoload": {
        "psr-4": {
            "advancedhosters\\assets\\advancedhosters\\": "src"
        }
    },
    "extra": {
        "config-plugin": {
            "params": "src/config/params.php"
        }
    }
}
